##########################
### dont_use cell list ###
##########################

# library suffix = <GP|LP><HVT|SVT|LVT>
#
# Drive too high -> electromigration & xtalk issue
#   CORE65<library suffix>    : *X106 *X142 *X213 *X284
#   CLOCK65<library suffix>   : *X82 *X103 *X124
#   CORL65LPHVT               : *Y106 *Y142 *Y213 *Y284
#   CORL65LPSVT               : *Y106 *Y142 *Y213 *Y284 *Y106_70 *Y142_70 *Y213_70 *Y284_70
#   HD65<library suffix>      : *X82 *X122 *X163
#   HDCLOCK65<library suffix> : *X74

#
# Drive too low -> timing not predictable due to a high sensitivity to routing
#   CORE65<library suffix>    : *X1 *X2
#   CLOCK65<library suffix>   : *X1 *X2
#   HD65<library suffix>      : *X1 *X2
#   HDI65<library suffix>     : *X1 *X2
#   HDCLOCK65<library suffix> : *X1 *X2
#   CORE65HV50                : *X1 *X2
#   CORL65LPHVT               : *Y1 *Y2
#   CORL65LPSVT               : *Y1 *Y2 *Y1_70 *Y2_70
#
# STM_techDontUseCellList is used only by MainStream flow (MDDP specific). The following cells are added to this variable to fix a convergence issue in ICCKIT.
#   CORL65LPHVT		  : *Y3 *Y4
#   CORL65LPSVT		  : *Y3 *Y4 *Y3_70 *Y4_70


if { ![info exists env(ICCKITROOT)] } {
set STM_techDontUseCell ""
foreach opcond {Worst Worst_10y Best Best_10y Nom NomLeak WorstPow bc_1.05V_105C bc_1.05V_125C bc_1.05V_m40C_10y bc_1.05V_m40C bc_1.10V_105C bc_1.10V_125C bc_1.10V_150C bc_1.10V_m40C_10y bc_1.10V_m40C bc_1.15V_105C bc_1.15V_125C bc_1.15V_m40C_10y bc_1.15V_m40C bc_1.25V_105C bc_1.25V_125C bc_1.25V_m40C_10y bc_1.25V_m40C bc_1.30V_105C bc_1.30V_125C bc_1.30V_150C bc_1.30V_m40C_10y bc_1.30V_m40C bc_1.35V_105C bc_1.35V_125C bc_1.35V_m40C_10y bc_1.35V_m40C nom_1.00V_25C nom_1.10V_25C nom_1.20V_25C nom_1.30V_25C wc_0.90V_105C_10y wc_0.90V_105C wc_0.90V_125C_10y wc_0.90V_125C wc_0.90V_150C_10y wc_0.90V_150C wc_0.95V_105C_10y wc_0.95V_105C wc_0.95V_125C_10y wc_0.95V_125C wc_1.00V_105C_10y wc_1.00V_105C wc_1.00V_125C_10y wc_1.00V_125C wc_1.05V_105C_10y wc_1.05V_105C wc_1.05V_125C_10y wc_1.05V_125C wc_1.10V_105C_10y wc_1.10V_105C wc_1.10V_125C_10y wc_1.10V_125C wc_1.10V_150C_10y wc_1.10V_150C wc_1.15V_105C_10y wc_1.15V_105C wc_1.15V_125C_10y wc_1.15V_125C wc_1.25V_105C_10y wc_1.25V_105C wc_1.25V_125C_10y wc_1.25V_125C bc_0.90V_m40C bc_0.90V_m40C_10y nom_0.80V_25C wc_0.70V_125C wc_0.70V_125C_10y bc_0.90V_125C} {
    foreach purpose {GP LP} {
        foreach process {HVT SVT LVT} {
            set librarySuffix ${purpose}${process}
            set STM_techDontUseCell [concat $STM_techDontUseCell [join "
                CORE65${librarySuffix}_${opcond}.db:CORE65${librarySuffix}/*X106
                CORE65${librarySuffix}_${opcond}.db:CORE65${librarySuffix}/*X142
                CORE65${librarySuffix}_${opcond}.db:CORE65${librarySuffix}/*X213
                CORE65${librarySuffix}_${opcond}.db:CORE65${librarySuffix}/*X284
                CLOCK65${librarySuffix}_${opcond}.db:CLOCK65${librarySuffix}/*X82
                CLOCK65${librarySuffix}_${opcond}.db:CLOCK65${librarySuffix}/*X103
                CLOCK65${librarySuffix}_${opcond}.db:CLOCK65${librarySuffix}/*X124
                CORE65${librarySuffix}_${opcond}.db:CORE65${librarySuffix}/*X1
                CORE65${librarySuffix}_${opcond}.db:CORE65${librarySuffix}/*X2
                CLOCK65${librarySuffix}_${opcond}.db:CLOCK65${librarySuffix}/*X1
                CLOCK65${librarySuffix}_${opcond}.db:CLOCK65${librarySuffix}/*X2
                HD65${librarySuffix}_${opcond}.db:HD65${librarySuffix}/*X82
                HD65${librarySuffix}_${opcond}.db:HD65${librarySuffix}/*X122
                HD65${librarySuffix}_${opcond}.db:HD65${librarySuffix}/*X163
                HDCLOCK65${librarySuffix}_${opcond}.db:HDCLOCK65${librarySuffix}/*X74
                HD65${librarySuffix}_${opcond}.db:HD65${librarySuffix}/*X1
                HD65${librarySuffix}_${opcond}.db:HD65${librarySuffix}/*X2
                HDCLOCK65${librarySuffix}_${opcond}.db:HDCLOCK65${librarySuffix}/*X1
                HDCLOCK65${librarySuffix}_${opcond}.db:HDCLOCK65${librarySuffix}/*X2
                HDI65${librarySuffix}_${opcond}.db:HDI65${librarySuffix}/*X1
                HDI65${librarySuffix}_${opcond}.db:HDI65${librarySuffix}/*X2
            "]]
        }
    }
    set STM_techDontUseCell [concat $STM_techDontUseCell [join "
        CORE65HV50_${opcond}.db:CORE65HV50/*X1
        CORE65HV50_${opcond}.db:CORE65HV50/*X2
    "]]
    set STM_techDontUseCell [concat $STM_techDontUseCell [join "
        CORL65LPHVT_${opcond}.db:CORL65LPHVT/*Y106
        CORL65LPHVT_${opcond}.db:CORL65LPHVT/*Y142
        CORL65LPHVT_${opcond}.db:CORL65LPHVT/*Y213
        CORL65LPHVT_${opcond}.db:CORL65LPHVT/*Y284
        CORL65LPHVT_${opcond}.db:CORL65LPHVT/*Y1
        CORL65LPHVT_${opcond}.db:CORL65LPHVT/*Y2
    "]]
     set STM_techDontUseCell [concat $STM_techDontUseCell [join "
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y106
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y106_70
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y142
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y142_70
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y213
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y213_70
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y284
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y284_70
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y1
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y1_70
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y2
        CORL65LPSVT_${opcond}.db:CORL65LPSVT/*Y2_70
    "]]
}}





set STM_techDontUseCellList ""
foreach purpose {GP LP} {
    foreach process {HVT SVT LVT} {
        set librarySuffix ${purpose}${process}
        set STM_techDontUseCellList [concat $STM_techDontUseCellList [join "
            CORE65${librarySuffix}/*X106
            CORE65${librarySuffix}/*X142
            CORE65${librarySuffix}/*X213
            CORE65${librarySuffix}/*X284
            CLOCK65${librarySuffix}/*X82
            CLOCK65${librarySuffix}/*X103
            CLOCK65${librarySuffix}/*X124
            CORE65${librarySuffix}/*X1
            CORE65${librarySuffix}/*X2
            CLOCK65${librarySuffix}/*X1
            CLOCK65${librarySuffix}/*X2
            HD65${librarySuffix}/*X82
            HD65${librarySuffix}/*X122
            HD65${librarySuffix}/*X163
            HDCLOCK65${librarySuffix}/*X74
            HD65${librarySuffix}/*X1
            HD65${librarySuffix}/*X2
            HDCLOCK65${librarySuffix}/*X1
            HDCLOCK65${librarySuffix}/*X2
            HDI65${librarySuffix}/*X1
            HDI65${librarySuffix}/*X2
        "]]
    }
}
set STM_techDontUseCellList [concat $STM_techDontUseCellList [join "
    CORE65HV50/*X1
    CORE65HV50/*X2
"]]
set STM_techDontUseCellList [concat $STM_techDontUseCellList [join "
    CORL65LPHVT/*Y106
    CORL65LPHVT/*Y142
    CORL65LPHVT/*Y213
    CORL65LPHVT/*Y284
    CORL65LPHVT/*Y1
    CORL65LPHVT/*Y2
"]]
 set STM_techDontUseCellList [concat $STM_techDontUseCellList [join "
    CORL65LPSVT/*Y106
    CORL65LPSVT/*Y106_70
    CORL65LPSVT/*Y142
    CORL65LPSVT/*Y142_70
    CORL65LPSVT/*Y213
    CORL65LPSVT/*Y213_70
    CORL65LPSVT/*Y284
    CORL65LPSVT/*Y284_70
    CORL65LPSVT/*Y1
    CORL65LPSVT/*Y1_70
    CORL65LPSVT/*Y2
    CORL65LPSVT/*Y2_70
"]]
set STM_techDontUseCellList [concat $STM_techDontUseCellList [join "
    CORL65LPHVT/*Y3
    CORL65LPHVT/*Y4
"]]
set STM_techDontUseCellList [concat $STM_techDontUseCellList [join "
    CORL65LPSVT/*Y3
    CORL65LPSVT/*Y3_70
    CORL65LPSVT/*Y4
    CORL65LPSVT/*Y4_70
"]]
